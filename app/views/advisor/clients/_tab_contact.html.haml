%h3
  = t('client.tab_a.tab_9')

=form.input :first_name, as: :string, :input_html => { :class => 'input' }
=form.input :last_name, as: :string, :input_html => { :class => 'input' }

=form.input :date_of_birth, label: 'DOB', as: :date, start_year: Time.zone.today.year, end_year: Time.zone.today.year - 70, :wrapper_html => { :class => 'with_multi_select' }, date_separator: '<span class="select"></span>', prompt: [], :order => [:day, :month, :year]

=form.input :email, as: :email, :input_html => { :class => 'input' }
=form.input :phone, as: :tel, :input_html => { :class => 'input' }

=form.input :address_line_1, as: :string, :input_html => { :class => 'input' }
=form.input :address_line_2, as: :string, :input_html => { :class => 'input' }
=form.input :postcode, as: :string, :input_html => { :class => 'input' }

=form.input :national_insurance_number, label: 'National Insurance Number', as: :tel, :input_html => { :class => 'input' }
